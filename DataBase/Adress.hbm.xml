<?xml version='1.0' encoding='utf-8'?>
        <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

<class name="DataBase.Adress" table="Adress" schema="dbo" catalog="master">
    <id name="id">
        <column name="ID" sql-type="int"/>
        <generator class="native" />
    </id>
    <property name="street">
        <column name="Street" sql-type="varchar(20)" length="20"/>
    </property>
    <property name="house">
        <column name="House" sql-type="varchar(20)" length="20"/>
    </property>
    <property name="city">
        <column name="City" sql-type="varchar(20)" length="20"/>
    </property>
    <property name="postCode">
        <column name="PostCode" sql-type="varchar(20)" length="20"/>
    </property>
</class>
</hibernate-mapping>