<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DataBase.RepairmentsEntity" table="Repairments" schema="dbo" catalog="master">
        <id name="id">
            <column name="ID" sql-type="int"/>
            <generator class="native" />
        </id>
        <property name="engine">
            <column name="Engine" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="tire">
            <column name="Tire" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="frontWindows">
            <column name="FrontWindows" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="doors">
            <column name="Doors" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="body">
            <column name="Body" sql-type="varchar(20)" length="20"/>
        </property>
        <many-to-one name="carByCarId" class="DataBase.CarEntity">
            <column name="Car_Id"/>
        </many-to-one>
    </class>
</hibernate-mapping>